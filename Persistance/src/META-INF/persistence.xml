<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">

    <persistence-unit name="NewPersistenceUnit">
        <class>entity.Caja</class>
        <class>entity.Categoria</class>
        <class>entity.Clientes</class>
        <class>entity.Estacion</class>
        <class>entity.Movimientos</class>
        <class>entity.Producto</class>
        <class>entity.ProductoAf</class>
        <class>entity.ProductoEe</class>
        <class>entity.Provedor</class>
        <class>entity.UnidadMedida</class>
        <class>entity.Usuario</class>
        <properties>
            <property name="toplink.jdbc.url" value="jdbc:mysql://localhost:3306"/>
